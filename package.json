{
  "name": "context-orchestrator",
  "version": "2.0.0",
  "description": "Multi-user web application to convert Fireflies.ai meeting transcripts into actionable tasks with intelligent deduplication and team collaboration via ClickUp",
  "type": "module",
  "main": "api/index.ts",
  "workspaces": [
    "web"
  ],
  "scripts": {
    "build": "tsc --outDir dist && npm run build:web",
    "build:web": "cd web && npm run build",
    "start": "vercel dev",
    "dev:api": "vercel dev --listen 3001",
    "dev:web": "cd web && npm run dev",
    "dev:all": "concurrently \"npm run dev:api\" \"npm run dev:web\"",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src api web/src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.ts\" \"api/**/*.ts\" \"web/src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "fireflies",
    "clickup",
    "task-management",
    "meeting-automation",
    "deduplication",
    "multi-user",
    "web-ui",
    "claude-ai"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.38.0",
    "@supabase/supabase-js": "^2.40.0",
    "dotenv": "^16.4.7",
    "express": "^5.2.1",
    "express-session": "^1.17.3",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.6"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/express-session": "^1.17.11",
    "@types/node": "^22.0.0",
    "@types/passport": "^1.0.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "vitest": "^1.1.0"
  }
}